{
  "manifest_version": 2,
  "name": "E文",
  "version": "0.1",
  "description": "抓取原文与译文",
  "background": { "scripts": ["background.js"] },
  "browser_action": {
    "default_icon": "icon.jpg"	
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],

	"content_scripts": [
	    {
	        "matches": [ "*://*/*" ],
	        "js":["jquery-1.9.1.min.js",
				"main.js",
				"ZeroClipboard.min.js"],
			"css":["main.css"],
	      "all_frames": true
	    }
	]
}